library(writexl)

set.seed(2022)
n <- 30

# consumo_diario - y
consumo_diario <- c(round(rnorm(n=250, mean = 1828.78, sd = 45),0))
#tiempo (en horas) - x1
tiempo <- c(round(rnorm(n = 250, mean = 90, sd = 30), 0))
#distancia - x2
distancia <- c(round(rnorm(n=250, mean = 3553.89, sd = 15),0))
#social - x3
rendimiento <- c(round(rnorm(n=250, mean = 1.29, sd = 0.5),2))


datos <- data.frame(bienestar, emocional, social, dinero)

datos


x1 <- tiempo
x2 <- distancia
x3 <- rendimiento
y <- consumo_diario



write_xlsx(data.frame(y, x1, x2, x3), path = "baseEstadistica.xlsx", col_names = TRUE)


