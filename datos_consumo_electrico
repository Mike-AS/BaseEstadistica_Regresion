library(writexl)

set.seed(2022)
n <- 30

# oxido nitroso - y
oxido_nitroso <- c(abs(round(rnorm(n=200, mean = 278.75, sd = 24.59906),0)))
#humedad - x1
humedad <- c(round(rnorm(n = 200, mean = 57.08333, sd = 19.40693), 0))
#temperatura - x2
temperatura <- c(round(rnorm(n=200, mean = 24.33333, sd = 1.302678),0))
#presion - x3
presion <- c(round(rnorm(n=200, mean = 89.33333, sd = 2.269695),0))
#pureza - x4
pureza <- c(round(rnorm(n=200, mean = 99.33333, sd = 6.429101),0))

datos <- data.frame(bienestar, emocional, social, dinero)

datos


x1 <- humedad
x2 <- temperatura
x3 <- presion
x4 <- pureza
y <- oxido_nitroso



write_xlsx(data.frame(y, x1, x2, x3, x4), path = "baseEstadistica.xlsx", col_names = TRUE)



